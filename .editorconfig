root = true

[*]
charset = utf-8
insert_final_newline = true

[*.cs]
end_of_line = crlf
indent_style = space
indent_size = 4

[*.{md,txt}]
indent_style = space
indent_size = 2

[*.html]
indent_style = space
indent_size = 2

[*{rc,config}]
end_of_line = lf

[*.bat]
end_of_line = crlf
charset = shiftjis
